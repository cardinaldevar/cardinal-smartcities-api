  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "production": "SET NODE_ENV=production&& concurrently \"npm run server\" \"npm run client\"",
    "production_aws": "NODE_ENV=production && concurrently \"npm run server\" \"npm run client\"",
    "aws-postbuild": "SET NODE_ENV=production && npm install --prefix client && npm run build --prefix client",
    "aws": "NPM_CONFIG_PRODUCTION=false concurrently \"npm start server\" \"npm start --prefix client\""
  },
